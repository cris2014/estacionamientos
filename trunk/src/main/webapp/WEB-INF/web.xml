<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Estacionamiento</display-name>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>

    <servlet>
         <servlet-name>LoginServlet</servlet-name>
         <servlet-class>pe.edu.upc.dew.estacionamiento.Controller.LoginServlet</servlet-class>
     </servlet>
     <servlet-mapping>
         <servlet-name>LoginServlet</servlet-name>
         <url-pattern>/LoginServlet</url-pattern>
     </servlet-mapping>

    <servlet>
         <servlet-name>LoginPasswordServlet</servlet-name>
         <servlet-class>pe.edu.upc.dew.estacionamiento.Controller.LoginPasswordServlet</servlet-class>
     </servlet>
     <servlet-mapping>
         <servlet-name>LoginPasswordServlet</servlet-name>
         <url-pattern>/LoginPasswordServlet</url-pattern>
     </servlet-mapping>



     <welcome-file-list>
         <welcome-file>default.jsp</welcome-file>
     </welcome-file-list>


    </web-app>
